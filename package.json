{
  "name": "prisma-nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "dev": "cross-env NODE_ENV=development nest start -b swc --watch ",
    "build": "nest build",
    "build:prod": "npm install && npx @nestjs/cli build",
    "start": "cross-env NODE_ENV=development nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:db": "npm run db:gen && npm run db:seed",
    "prisma:studio": "npx prisma studio",
    "db:build:prod": "npm run db:migrate:dev && npm run db:push && npm run db:reset:force && npm run db:seed",
    "db:seed": "npx ts-node -T ./prisma/seed.ts",
    "db:push": "npx prisma db push --accept-data-loss",
    "db:seed:create": "hygen seeds create",
    "db:reset:full": "powershell ./reset-db.ps1",
    "db:reset:no:force": "prisma migrate reset --skip-seed",
    "db:reset:force": "prisma migrate reset --force",
    "db:create": "npm run db:reset:force && npm run db:seed",
    "db:migrate:dev": "npx prisma migrate dev --skip-seed",
    "db:migrate:dev:create": "npx prisma migrate dev --create-only",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:migrate:status": "npx prisma migrate status",
    "db:migrate:resolve": "npx prisma migrate resolve",
    "db:gen": "npx prisma generate",
    "db:gen:watch": "npx prisma generate --watch",
    "db:gen:static": "npx ts-node prisma/helpers/generateStaticDataFromDb.ts && prettier --write \"prisma/**/staticDataFromDb.ts\"",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "redis:create": "docker run --name my-redis -p 6379:6379 -d redis",
    "redis:start": "docker start my-redis",
    "redis:stop": "docker stop my-redis",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint --fix prisma/dataFromDb.ts",
    "test": "cross-env NODE_ENV=development jest --config test/jest-e2e.ts  --verbose -T --detectOpenHandles",
    "test:pipe": "powershell ./test-pipe.ps1",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "postinstall": "",
    "prepare": "is-ci || husky install",
    "update-package-lock": "powershell ./update-package-lock.ps1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.456.0",
    "@nestjs/common": "10.2.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.10",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.2",
    "@nestjs/platform-express": "10.2.10",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "7.1.16",
    "@nestjs/typeorm": "10.0.1",
    "@types/multer-s3": "3.0.3",
    "apple-signin-auth": "1.7.5",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "fb": "2.0.0",
    "google-auth-library": "9.2.0",
    "handlebars": "4.7.8",
    "ioredis": "^5.3.2",
    "kill-port": "^2.0.1",
    "ms": "2.1.3",
    "multer": "1.4.5-lts.1",
    "multer-s3": "3.0.1",
    "nestjs-i18n": "10.3.7",
    "nestjs-prisma": "^0.22.0",
    "nodemailer": "6.9.7",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "passport-local": "^1.0.0",
    "pg": "8.11.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "5.0.0",
    "twitter": "1.7.1",
    "@nestjs/cli": "10.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.2.10",
    "@prisma/client": "^5.6.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "4.17.21",
    "@types/facebook-js-sdk": "3.3.10",
    "@types/jest": "29.5.10",
    "@types/ms": "0.7.34",
    "@types/multer": "1.4.11",
    "@types/node": "20.9.4",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "2.0.16",
    "@types/twitter": "1.7.4",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "env-cmd": "10.1.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "fs": "0.0.1-security",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-html-reporters": "^3.1.4",
    "prettier": "3.1.0",
    "prisma": "^5.6.0",
    "supertest": "6.3.3",
    "swc-loader": "^0.2.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.6.2",
    "typescript": "5.3.2"
  },
  "prisma": {
    "seed": "ts-node -T ./prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}