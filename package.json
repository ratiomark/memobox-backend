{
  "name": "prisma-nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:db": "npm run db:generate && npm run db:seed",
    "prisma:studio": "npx prisma studio",
    "db:seed": "npx ts-node -T prisma/seed.ts",
    "db:push": "npx prisma db push",
    "db:seed:create": "hygen seeds create",
    "db:reset:force": "prisma migrate reset --skip-seed --force",
    "db:create": "npm run db:reset:force && npm run db:seed",
    "db:migrate:dev": "prisma migrate dev --preview-feature",
    "db:migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "db:migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "db:migrate:status": "npx prisma migrate status --preview-feature",
    "db:migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "db:generate": "npx prisma generate",
    "db:generate:watch": "npx prisma generate --watch",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "cross-env NODE_ENV=development jest --config test/jest-e2e.ts --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "postinstall": "npm run db:generate",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.428.0",
    "@nestjs/common": "10.2.7",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.7",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/passport": "10.0.2",
    "@nestjs/platform-express": "10.2.7",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "7.1.13",
    "@nestjs/typeorm": "10.0.0",
    "@types/multer-s3": "3.0.1",
    "apple-signin-auth": "1.7.5",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "fb": "2.0.0",
    "google-auth-library": "9.1.0",
    "handlebars": "4.7.8",
    "ms": "2.1.3",
    "multer": "1.4.4",
    "multer-s3": "3.0.1",
    "nestjs-i18n": "10.3.6",
    "nestjs-prisma": "^0.22.0",
    "nodemailer": "6.9.6",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "passport-local": "^1.0.0",
    "pg": "8.11.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "5.0.0",
    "twitter": "1.7.1",
    "typeorm": "0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.7",
    "@prisma/client": "^5.5.2",
    "@types/bcryptjs": "2.4.4",
    "@types/express": "4.17.18",
    "@types/facebook-js-sdk": "3.3.8",
    "@types/jest": "29.5.5",
    "@types/ms": "0.7.32",
    "@types/multer": "1.4.8",
    "@types/node": "18.18.1",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.10",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "2.0.13",
    "@types/twitter": "1.7.2",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "env-cmd": "10.1.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-html-reporters": "^3.1.4",
    "prettier": "3.0.3",
    "prisma": "^5.5.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}